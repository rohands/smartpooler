#include <stdio.h>
#include <unistd.h>
#include <stdlib.h>
#include <time.h>

int main()
{
	int pid;
	int c = 0;
	int d = 0;
	pid = fork();
	if(pid == 0)
	{
		c = 97;
		while(c <= 122)
		{
			printf("%c\n", c);
			++c;
			sleep(rand() % 3);
		}
		
	}
	else
	{
		d = 65;
		while(d <= 90)
		{
			printf("%c\n", d);
			++d;
			sleep(rand() % 3);
		}
	}
    //printf("abcd\n");
	//fork();
	//printf("pqrs\n");
	return 0;
}
